# GitHub
GITHUB_TOKEN=ghp_your_token_here
GITHUB_ORG=your-org-name
GITHUB_USERNAME=your-github-username

# Atlassian (Confluence & Jira)
ATLASSIAN_URL=https://your-company.atlassian.net
ATLASSIAN_EMAIL=your-email@company.com
ATLASSIAN_API_TOKEN=your_api_token_here
ATLASSIAN_ACCOUNT_ID=your_atlassian_account_id
ATLASSIAN_CLOUD_ID=your_cloud_id  # Get from: curl https://your-company.atlassian.net/_edge/tenant_info
# CONFLUENCE_URL=https://your-confluence.atlassian.net  # Optional: if Confluence is on a different tenant

# Greenhouse (optional)
# GREENHOUSE_API_KEY=your_greenhouse_api_key
# GREENHOUSE_USER_ID=your_greenhouse_user_id  # Your user ID for filtering interviews

# AI Summaries (optional, disabled by default)
# ENABLE_AI_SUMMARIES=true

# Note: You have Anthropic API directly/LiteLLM proxy optionality
# ANTHROPIC_API_KEY=sk-ant-your_key_here
# ANTHROPIC_BASE_URL=https://your-litellm-proxy.com  # Optional: for LiteLLM/Bedrock proxy
